    "mitt": "^3.0.0",
    "moment": "^2.29.4",
    "socket.io-client": "^4.6.1",
    "sockjs-client": "^1.6.1",
    "vue": "^3.2.13",
    "vue-moment": "^4.1.0",
    "vue-router": "^4.1.6",
    "vuex": "^4.1.0",
    "vuex-persist": "^3.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
